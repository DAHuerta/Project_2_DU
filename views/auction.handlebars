<div id="auction-frame">
    {{#each cars}}
    {{#if auction}}<br>
    Model: {{model}}<br>
    Make: {{make}}<br>
    Year: {{year}}<br>
    Color: {{color}}<br>
    <img src={{image}}><br>
    Highest Bid:
    <div id="currentBid">
    ${{bid}}<br>
    </div>
    Highest Bidder:
    <div id="currentBidder">
    {{currentBidder}}<br>
    </div>
    <form>
        <input type="number" id="bid"></input>
        <input type="text" id="bidder"></input>
        <button id="auctionId" data-value="{{id}}">Submit!</button>
    </form>
    {{/if}}
    {{/each}}
</div>